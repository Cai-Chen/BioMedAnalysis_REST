<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <name>Basic Type for the Annotator</name>
  <description>A basic type system to store the output of the Annotator.</description>
  <version>1.0</version>
  <vendor/>
  <types>
    <typeDescription>
      <name>com.unimelb.comp90055.bmAnalysis.type.Candidate</name>
      <description>The meta elements found in the text. Those are then used as candidates and mappings.</description>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>score</name>
          <description>The score of this meta element (0 &lt;= x &lt;= 1000).</description>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>spans</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>com.unimelb.comp90055.bmAnalysis.type.Span</elementType>
        </featureDescription>
        <featureDescription>
          <name>cui</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>concept</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>preferred</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>head</name>
          <description/>
          <rangeTypeName>uima.cas.Boolean</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>overmatch</name>
          <description/>
          <rangeTypeName>uima.cas.Boolean</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>sources</name>
          <description/>
          <rangeTypeName>uima.cas.StringArray</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>semanticTypes</name>
          <description/>
          <rangeTypeName>uima.cas.StringArray</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>matchedwords</name>
          <description/>
          <rangeTypeName>uima.cas.StringArray</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>matchMap</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>com.unimelb.comp90055.bmAnalysis.type.MatchMap</elementType>
        </featureDescription>
      <featureDescription>
          <name>atoms</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>com.unimelb.comp90055.bmAnalysis.type.Atom</elementType>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>com.unimelb.comp90055.bmAnalysis.type.Mapping</name>
      <description>A meta mapping.</description>
      <supertypeName>uima.cas.TOP</supertypeName>
      <features>
        <featureDescription>
          <name>score</name>
          <description>The score of this mapping.</description>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>candidates</name>
          <description>An array of elements of this mapping.</description>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>com.unimelb.comp90055.bmAnalysis.type.Candidate</elementType>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>com.unimelb.comp90055.bmAnalysis.type.Phrase</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>candidates</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>com.unimelb.comp90055.bmAnalysis.type.Candidate</elementType>
        </featureDescription>
        <featureDescription>
          <name>mappings</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>com.unimelb.comp90055.bmAnalysis.type.Mapping</elementType>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>com.unimelb.comp90055.bmAnalysis.type.Span</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>com.unimelb.comp90055.bmAnalysis.type.Utterance</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>pmid</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>phrases</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>com.unimelb.comp90055.bmAnalysis.type.Phrase</elementType>
        </featureDescription>
        <featureDescription>
          <name>location</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>com.unimelb.comp90055.bmAnalysis.type.Negation</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>negType</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>negTrigger</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>ntSpans</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>com.unimelb.comp90055.bmAnalysis.type.Span</elementType>
        </featureDescription>
        <featureDescription>
          <name>cuiConcepts</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>com.unimelb.comp90055.bmAnalysis.type.CuiConcept</elementType>
        </featureDescription>
        <featureDescription>
          <name>ncSpans</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>com.unimelb.comp90055.bmAnalysis.type.Span</elementType>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>com.unimelb.comp90055.bmAnalysis.type.CuiConcept</name>
      <description/>
      <supertypeName>uima.cas.TOP</supertypeName>
      <features>
        <featureDescription>
          <name>negExCui</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>negExConcept</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>com.unimelb.comp90055.bmAnalysis.type.MatchMap</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>phraseMatchStart</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>phraseMatchEnd</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>conceptMatchStart</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>conceptMatchEnd</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>lexVariation</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  <typeDescription>
      <name>com.unimelb.comp90055.bmAnalysis.type.Atom</name>
      <description>Retrieve atoms from Metathesaurus of UMLS according to cui</description>
      <supertypeName>uima.cas.TOP</supertypeName>
      <features>
        <featureDescription>
          <name>aui</name>
          <description>AUI</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>name</name>
          <description>Name</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>language</name>
          <description>Source Language</description>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
          <name>com.unimelb.comp90055.bmAnalysis.type.AcronymAbbrev</name>
          <description/>
          <supertypeName>uima.tcas.Annotation</supertypeName>
          <features>
            <featureDescription>
              <name>acronym</name>
              <description/>
              <rangeTypeName>uima.cas.String</rangeTypeName>
            </featureDescription>
            <featureDescription>
              <name>expansion</name>
              <description/>
              <rangeTypeName>uima.cas.String</rangeTypeName>
            </featureDescription>
            <featureDescription>
              <name>countList</name>
              <description/>
              <rangeTypeName>uima.cas.IntegerArray</rangeTypeName>
            </featureDescription>
            <featureDescription>
              <name>cuiList</name>
              <description/>
              <rangeTypeName>uima.cas.StringArray</rangeTypeName>
            </featureDescription>
          </features>
        </typeDescription>
        <typeDescription>
          <name>com.unimelb.comp90055.bmAnalysis.type.Document</name>
          <description/>
          <supertypeName>uima.tcas.Annotation</supertypeName>
          <features>
            <featureDescription>
              <name>acronymAbbrevs</name>
              <description/>
              <rangeTypeName>uima.cas.FSArray</rangeTypeName>
              <elementType>com.unimelb.comp90055.bmAnalysis.type.AcronymAbbrev</elementType>
            </featureDescription>
            <featureDescription>
              <name>negations</name>
              <description/>
              <rangeTypeName>uima.cas.FSArray</rangeTypeName>
              <elementType>com.unimelb.comp90055.bmAnalysis.type.Negation</elementType>
            </featureDescription>
            <featureDescription>
              <name>utterances</name>
              <description/>
              <rangeTypeName>uima.cas.FSArray</rangeTypeName>
              <elementType>com.unimelb.comp90055.bmAnalysis.type.Utterance</elementType>
            </featureDescription>
          </features>
        </typeDescription>
  </types>
</typeSystemDescription>
